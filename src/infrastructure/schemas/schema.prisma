generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id        Int      @id @default(autoincrement())
    name      String  
    email     String   @unique
    password  String

     @@map("users")
}

model ExchangeRegistration {
    id                Int      @id @default(autoincrement())
    acronym           String
    code_source       String  
    code_destination  String  
    value             Float
    date              String

  @@map("exchangeregistration")
}